name: RESAdministrators
sAMAccountName: RESAdministrators
objectCategory: CN=Group,CN=Schema,CN=Configuration,DC=${DC}
gidNumber: 1005
member: CN=admin1,OU=Users,OU=RES,OU=${OU},DC=${DC}
